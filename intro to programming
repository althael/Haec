package exerciseJavasum;

public class  exercise_java {
	
	public static int sum_integer(int a,int b) {
	
	int sum_result=a+b;
	
	return sum_result;
	
	}

	public static int diff_integer(int a,int b) {
		
		int diff_result=a-b;
		
		return diff_result;
		
		
	}
	
	
public static void main(String[] args) {
	int a=1;
	int b=2;
	
	
	
    System.out.println("calling sum_integer");
	sum_integer(a,b);
	
	System.out.println(sum_integer);
	
	System.out.println("calling diff_integer");
	diff_integer(a,b);
	
	System.out.println(diff_integer);
}
	
}
