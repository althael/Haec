
public class Bubblesort {

	static int[] unsortedArray = { 1, 2, 3 };

	public static void Bubblesort_Medthod(int[] num) {

		boolean unsorted = true;
		while (unsorted) {
			unsorted = false;
			for (int i = 0; i < unsortedArray.length - 1; i++) {
				if (unsortedArray[i] > unsortedArray[i + 1]) {
					int temp = unsortedArray[i + 1];
					unsortedArray[i + 1] = unsortedArray[i];
					unsortedArray[i] = temp;
					unsorted = true;

				}

			}

		}

	}

	public static void main(String[] args) {

		System.out.println("Prints the unsorted array" + unsortedArray);
		System.out.println("Prints the sorted array" + sortedArray);

	}
}
